// Templates & General Swithes
    template tmpl_purchase(x, y) {
        [x,      y,   75,     15,   -39,   -9]
    }

    template tmpl_purchase_long(x, y) {
        [x,      y,   100,     15,   -39,   -9]
    }

    template tmpl_vehicle(x, y) {
        [x,      y,     8,    34,    -3,  -18] // ↑
        [x+20,   y,    29,    24,   -17,  -14] // ↗
        [x+60,   y,    48,    15,   -24,  -11] // →
        [x+120,  y,    29,    24,   -10,  -14] // ↘
        [x+160,  y,     8,    34,    -3,  -18] // ↓
        [x+180,  y,    29,    24,   -17,  -14] // ↙
        [x+220,  y,    48,    15,   -24,  -11] // ←
        [x+280,  y,    29,    24,   -10,  -14] // ↖
    }

    template tmpl_vehicle_empty(x, y) {
        [x,      y,     1,     1,     0,    0] // ↑
        [x,      y,     1,     1,     0,    0] // ↗
        [x,      y,     1,     1,     0,    0] // →
        [x,      y,     1,     1,     0,    0] // ↘
        [x,      y,     1,     1,     0,    0] // ↓
        [x,      y,     1,     1,     0,    0] // ↙
        [x,      y,     1,     1,     0,    0] // ←
        [x,      y,     1,     1,     0,    0] // ↖
    }

