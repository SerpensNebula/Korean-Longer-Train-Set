    grf {
        grfid                              :"KLTS"; 
        name                               :string(STR_GRF_NAME);
        desc                               :string(STR_GRF_DESC);
        url                                :string(STR_GRF_URL);
        version                            :250531;
        min_compatible_version             :250204;

        // Parameters
            param  0    {
                para_speed_power {
                    type:       int;
                    name:       string(STR_PARA_NAME_SPEED_POWER);
                    desc:       string(STR_PARA_DESC_SPEED_POWER);
                    min_value:  2;
                    max_value:  4;
                    def_value:  2;
                        
                    names: {
                        2:      string(STR_PARA_SPEED_POWER_OPT_1);
                        3:      string(STR_PARA_SPEED_POWER_OPT_1_5);
                        4:      string(STR_PARA_SPEED_POWER_OPT_2);
                    };
                }
            }

            param  1    {
                para_capacity {
                    type:       int;
                    name:       string(STR_PARA_NAME_CAPA);
                    desc:       string(STR_PARA_DESC_CAPA);
                    min_value:  1;
                    max_value:  2;
                    def_value:  1;
                        
                    names: {
                        1:      string(STR_PARA_CAPA_OPT_1);
                        2:      string(STR_PARA_CAPA_OPT_2);
                    };
                }
            }
    }

